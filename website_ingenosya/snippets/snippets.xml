<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="rdlp_snippets" inherit_id="website.snippets">
        <xpath expr="//div[@id='o_scroll']" position="inside">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">
                    <i class="fa fa-th-large" />Snippets Ingenosya
                </div>
                <div class="o_panel_body">
                    <t t-snippet="website_ingenosya.s_header_slider" t-thumbnail="/website_ingenosya/static/src/img/snippets/slide1.png" />
                    <t t-snippet="website_ingenosya.s_bref" t-thumbnail="/website_ingenosya/static/src/img/snippets/logoV.png" />
                    <t t-snippet="website_ingenosya.s_icone" t-thumbnail="/website_ingenosya/static/src/img/snippets/slide.png" />
                    <t t-snippet="website_ingenosya.s_specialit_titre" t-thumbnail="/website_ingenosya/static/src/img/snippets/title_specialites1.png" />
                    <t t-snippet="website_ingenosya.s_contenu_splte_1" t-thumbnail="/website_ingenosya/static/src/img/pages/web.png" />
                    <t t-snippet="website_ingenosya.s_contenu_splte_2" t-thumbnail="/website_ingenosya/static/src/img/pages/web.png" />
                    <t t-snippet="website_ingenosya.s_titre_techonologie" t-thumbnail="/website_ingenosya/static/src/img/pages/web.png" />
                    <t t-snippet="website_ingenosya.s_contenu_techonologie" t-thumbnail="/website_ingenosya/static/src/img/pages/web.png" />
                    <t t-snippet="website_ingenosya.s_recrutement" t-thumbnail="/website_ingenosya/static/src/img/snippets/recru1.png" />
                    <t t-snippet="website_ingenosya.s_outil_de_developpement" t-thumbnail="/website_ingenosya/static/src/img/snippets/outil.png" />
                    <t t-snippet="website_ingenosya.s_titre_outil" t-thumbnail="/website_ingenosya/static/src/img/snippets/title_outil.png" />
                    <t t-snippet="website_ingenosya.s_actualite" t-thumbnail="/website_ingenosya/static/src/img/actualite/actualite.png" />
                </div>
            </div>
        </xpath>
    </template>
</odoo>
